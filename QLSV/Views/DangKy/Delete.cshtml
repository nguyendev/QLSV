@model QLSV.Models.DangKy

@{
    ViewData["Title"] = "Index";
    ViewData["Heading"] = "Đăng ký";
    ViewData["SmallHeading"] = "";
    ViewData["BreadcrumbOne"] = "QLSV";
    ViewData["BreadcrumbTwo"] = "Đăng ký";
    ViewData["BreadcrumbThree"] = "Xóa";
    ViewData["BoxTitle"] = "Xóa";
    ViewData["Search"] = "Tìm học phần";
    Layout = "_TableIndexLayout";
}
<h3>Bạn có muốn xóa không?</h3>
<div class="box-body">
   
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.MahpNavigation.Mahp)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MahpNavigation.Mahp)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.MssvNavigation.Mssv)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MssvNavigation.Mssv)
        </dd>
    </dl>
</div>
<div class="box-footer">
    <form asp-action="Delete">
        <input type="hidden" asp-for="Mahp" />
        <input type="hidden" asp-for="Mssv" />
        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-danger pull-right" /> 
            <a class="btn btn-default pull-left" asp-action="Index">Quay lai</a>
        </div>
    </form>
    
</div>

