@{
    Layout = "_Layout";
    
}
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            @ViewData["Heading"]
            <small>@ViewData["SmallHeading"]</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> @ViewData["BreadcrumbOne"]</a></li>
            <li><a href="#">@ViewData["BreadcrumbTwo"]</a></li>
            <li class="active">@ViewData["BreadcrumbThree"]</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">@ViewData["BoxTitle"]</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        
                            @RenderBody()
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
            </div>
        </div>
    </section>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script>
        $(function () {


            //Date picker
            $('.datepicker').datepicker({
                autoclose: true
            });

        });
    </script>
}